{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "preinstall": {},
    "prebuild": {
      "dependsOn": ["^prebuild", "preinstall"]
    },
    "build": {
      "dependsOn": ["^build", "prebuild"],
      "outputs": ["dist/**", "example-contract/src/types/**"]
    },
    "pretest": {
      "outputMode": "new-only"
    }
  }
}
