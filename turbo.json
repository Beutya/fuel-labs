{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "preinstall": {
      "outputMode": "new-only"
    },
    "prebuild": {
      "dependsOn": ["^prebuild", "preinstall"],
      "outputMode": "new-only"
    },
    "build": {
      "dependsOn": ["^build", "prebuild"],
      "outputs": ["dist/**", "example-contract/src/types/**"],
      "outputMode": "new-only"
    },
    "postbuild": {
      "dependsOn": ["^postbuild", "build"],
      "outputMode": "new-only"
    },
    "pretest": {
      "outputMode": "new-only"
    },
    "test": {
      "dependsOn": ["^test", "pretest"],
      "outputMode": "new-only"
    }
  }
}
