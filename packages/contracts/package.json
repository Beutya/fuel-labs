{
  "name": "@fuel-ts/contracts",
  "version": "0.22.0",
  "license": "Apache-2.0",
  "description": "Enable developers to build, deploy, generate types and replace contracts ids",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "typedoc": {
    "entryPoint": "src/index.ts"
  },
  "bin": {
    "fuels-contracts": "dist/bin.global.js"
  },
  "exports": {
    ".": "./src/index.ts",
    "./cli": "./src/bin/cli.ts"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
      "fuels-contracts": "dist/bin.global.js"
    },
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./cli": {
        "import": "./dist/cli.mjs",
        "require": "./dist/cli.js",
        "types": "./dist/cli.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --dts",
    "build:watch": "tsup --dts --watch"
  },
  "dependencies": {
    "@ethersproject/bytes": "^5.7.0",
    "@fuel-ts/abi-coder": "workspace:*",
    "@fuel-ts/abi-typegen": "workspace:*",
    "@fuel-ts/contract": "workspace:*",
    "@fuel-ts/providers": "workspace:*",
    "@fuel-ts/transactions": "workspace:*",
    "@fuel-ts/versions": "workspace:*",
    "@fuel-ts/wallet": "workspace:*",
    "@types/lodash.camelcase": "^4.3.7",
    "bundle-require": "^3.1.2",
    "chalk": "4",
    "commander": "^9.4.1",
    "esbuild": "^0.16.16",
    "joycon": "^3.1.1",
    "lodash.camelcase": "^4.3.0",
    "toml": "^3.0.0",
    "typescript": "^4.8.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@fuel-ts/address": "workspace:*",
    "@types/rimraf": "^3.0.2",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3"
  }
}
