{
  "verbose": true,
  "ext": "ts",
  "watch": "packages",
  "ignore": [
    "**/*.d.ts",
    "**/*.map",
    "**/*.test.ts",
    "**/*.js",
    "**/out/debug/**",
    "packages/example-contract",
    "packages/fuel-gauge",
    "packages/providers/src/__generated__",
    "packages/testcases"
  ],
  "exec": "turbo run build --filter=!fuel-gauge"
}
